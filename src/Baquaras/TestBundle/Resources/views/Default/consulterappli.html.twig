					<h2> Historique de l'avancement de la qualification </h2>
							
					<div class="content" id="statut">
					<table>
						<tr>
							<td id="label"> Statut </td>
							<td id="text"> {% for package in application.packages %} {{ package.statutQualif.libelle }} {% endfor %} </td>
						</tr>
					</table>
				</li>
                            {% endif %}
			</ul>
                        {#% fin onglet qualification %#}                      
			{% endif %}
                        {#% debut onglet Mise à jour %#}
                        {% if(getAuthorisation('onglet mise a jour')) == true %}
                            <ul id="accordion_maj" class="accordion">
                                {% if(getAuthorisation('Item Mises a jour')) == true %}
                                    <li>
                                            <a id="maj_accordion_plus" class="icone"> <img src="{{ asset('bundles/baquaras/images/plus.jpg') }}" height=20 width=20> </a> 
                                            <a id="maj_accordion_moins" class="icone"> <img src="{{ asset('bundles/baquaras/images/moins.jpg') }}" height=20 width=20> </a>					
                                            <h2> Mises à jour </h2>

                                            <div class="content" id="maj">
                                                    {% if application.misesajour is empty %}
                                                            <div class="text"> Pas de mise à jour </td>
                                                    {% endif %}

                                                    {% for miseajour in application.misesajour %}
                                                    <h3> <a id="maj_accordion{{miseajour.id}}"> + Mise à jour n° {{ miseajour.id }} </a> </h3>

                                                    <table id="maj{{miseajour.id}}" class="maj">	
                                                            <tr>
                                                                    <td id="label"> Type de mise à jour </td>
                                                                    <td> {{ miseajour.type }} </td>
                                                            </tr>
                                                            <tr>
                                                                    <td id="label"> Version </td>
                                                                    <td> {{ miseajour.version }} </td>
                                                            </tr>
                                                            <tr>
                                                                    <td id="label"> Indice </td>
                                                                    <td> {{ miseajour.indice }} </td>
                                                            </tr>
                                                            <tr>
                                                                    <td id="label"> OS cible </td>
                                                                    <td> {{ miseajour.oscible }} </td>
                                                            </tr>
                                                            <tr>
                                                                    <td id="label"> Statut du patch </td>
                                                                    <td> {{ miseajour.statutPatch }} </td>
                                                            </tr>
                                                            <tr>
                                                                    <td id="label"> Description </td>
                                                                    <td> {{ miseajour.description }} </td>
                                                            </tr>
                                                            {% if is_granted('IS_AUTHENTICATED_FULLY') %}
                                                                {% if app.session and app.session.get('role') in ['Administrateur',
                                                                                                            'Responsable qualification',
                                                                                                            'Intégrateur',
                                                                                                            'Chef de produit'
                                                                                                          ] %}
                                                                    <tr>
                                                                            <td id="label"> Date de demande de mise à jour </td>
                                                                            <td> 
                                                                                    {% if miseajour.dateDemandeMAJ is not null %}
                                                                                            {{ miseajour.dateDemandeMAJ|date('d.m.Y') }} 
                                                                                    {% endif%}
                                                                            </td>
                                                                    </tr>
                                                                    <tr>
                                                                        <td id="label"> Documentation technique </td>
                                                                        <td> {{ miseajour.documentationTechnique }} </td>
                                                                    </tr>
                                                                    <tr>
                                                                        <td id="label"> Date de mise en production de la mise à jour </td>
                                                                        <td> 
                                                                                {% if miseajour.dateMiseEnProdMAJ is not null %}
                                                                                        {{ miseajour.dateMiseEnProdMAJ|date('d.m.Y') }} 
                                                                                {% endif %}
                                                                        </td>
                                                                    </tr>
                                                                    <tr>
                                                                    <td id="label"> Personne chargée de la mise à jour </td>
                                                                    <td> {{ miseajour.personneChargeeMAJ }} </td>
                                                                    </tr>
                                                                    <tr>
                                                                    <td id="label"> Nom de publication du patch </td>
                                                                    <td> {{ miseajour.nomPublicationPatch }} </td>
                                                                </tr>
                                                                <tr>
                                                                    <td id="label"> Ligne de commande pour la télédistribution du patch </td>
                                                                    <td> {{ miseajour.ligneCommandePatchTeledistribution }} </td>
                                                                </tr>
                                                                <tr>
                                                                    <td id="label"> Ligne de commande pour la publication du patch </td>
                                                                    <td> {{ miseajour.ligneCommandePatchPublication }} </td>
                                                                </tr>
                                                                <tr>
                                                                    <td id="label"> Chemin du patch </td>
                                                                    <td> {{ miseajour.cheminPatch }} </td>
                                                                </tr>
                                                                {% endif %}
                                                            {% endif %}
                                                    </table>
                                                    {% endfor %}
                                                    </li>
                                                {% endif %}
                            </ul>
                    {#% fin onglet mis a jour %#}
                    {% endif %}
{#%***************************************************************************************************************************************************************************%#}						
