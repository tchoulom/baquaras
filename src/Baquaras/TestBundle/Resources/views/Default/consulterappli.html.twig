                            {% endif %}
                            {% if(getAuthorisation('avancement de la qualification')) == true %}
				<li>
					<a id="statut_accordion_plus" class="icone"> <img src="{{ asset('bundles/baquaras/images/plus.jpg') }}" height=20 width=20> </a> 
