<!DOCTYPE html>

<html>
			
	<head>			
		<title> Caractéristiques de l'application {{application.nom}} </title>
		{% include 'BaquarasTestBundle:Default:css.html.twig' %}
	</head>
		
	<body>
		
		{% include 'BaquarasTestBundle:Default:onglets.html.twig' %}
		{% include 'BaquarasTestBundle:Default:jquery2.html.twig' %}
		

		
		<div class="consulterappli">

                    <h1> Caractéristiques d'une application </h1>

                    <div id="onglet">
                        {% if(getAuthorisation('onglet application')) == true %}
                            <div id="infos_appli"> <a id="infos_appli_onglet"> Application </a> </div>
                        {% endif %}
                        {% if(getAuthorisation('onglet package')) == true %}
                            <div id="infos_package"> <a id="infos_package_onglet"> Package </a> </div>
                        {% endif %}
                        {% if(getAuthorisation('onglet qualification')) == true %}
                            <div id="infos_qualif"> <a id="infos_qualif_onglet"> Qualification </a> </div>
                        {% endif %}
                        {% if(getAuthorisation('onglet mise a jour')) == true %}
                            <div id="infos_maj"> <a id="infos_maj_onglet"> Mises à jour </a> </div>
                        {% endif %}
                    </div>
			{#% debut application %#}
                        {% if(getAuthorisation('onglet application')) == true %}
                            <ul id="accordion_appli" class="accordion">
                                {% if(getAuthorisation('Informations sur l\'application')) == true %}
                                    <li>
                                            <a id="infogenerales_accordion_plus" class="icone"> <img src="{{ asset('bundles/baquaras/images/plus.jpg') }}" height=20 width=20> </a> 
                                            <a id="infogenerales_accordion_moins" class="icone"> <img src="{{ asset('bundles/baquaras/images/moins.jpg') }}" height=20 width=20> </a>
                                            <h2> Informations sur l'application </h2>
                                            <div class="content" id="infogenerales">
                                            <table>
                                                    <tr>	
                                                    <td id="label"> <div class="label"> Nom </div>
                                                            <a title="Nom usuel de l'application" href="#"> <div class="bulle"> </div> </a>
                                                    </td>
                                                    <td id="text"> {{ application.nom }} </td>
                                                    </tr>
                                                    <tr>
                                                    <td id="label"> <div class="label"> Editeur </div>
                                                            <a title="Nom de l'éditeur s'il s'agit d'un progiciel" href="#"> <div class="bulle"> </div> </a>
                                                    </td> 
                                                    <td id="text"> {{ application.editeur }} </td>
                                                    </tr>
                                                    <tr> 
                                                    <td id="label"> Version </td>
                                                    <td id="text"> {{ application.version }} </td>
                                                    </tr>
                                                    <tr> 
                                                    <td id="label"> Type de l'application </td>
                                                    <td id="text">
                                                            {{ application.type }}
                                                    </td>
                                                    </tr>
                                                    <tr> 
                                                            <td id="label"> <div class="label"> Description </div>
                                                                    <a title="Description succincte de l'application" href="#"> <div class="bulle"> </div> </a>
                                                            </td> 
                                                    <td id="text"> {{ application.description }} </td>
                                                    </tr>
                                                    <tr> 
                                                    <td id="label"> Correctif Qualif </td> 
                                                    <td id="text"> {{ application.correctifQualif }} </td>
                                                    </tr>
                                                    <tr>
                                                    <td id="label"> <div class="label"> Code Convergence </div> 
                                                            <a title="Code de l'application interne SIT" href="#"> <div class="bulle"> </div> </a>
                                                    </td>
                                                    <td id="text"> {{ application.codeConvergence }} </td>
                                            </tr>
                                            <tr>
                                                    <td id="label"> Code ECAR </td>	
                                                    <td id="text"> {{ application.codeECAR }} </td>
                                            </tr>
                                            <tr> 
                                                    <td id="label"> Référencée dans SIERA </td> 
                                                    <td id="text"> 
                                                            {% if application.appliReferenceeSIERA is not null %}
                                                                    {% if application.appliReferenceeSIERA==1 %} Oui {% endif %}
                                                                    {% if application.appliReferenceeSIERA==0 %} Non {% endif %}
                                                            {% endif %}
                                                    </td>
                                            </tr>
                                            <tr>
                                                    <td id="label"> Code SIERA </td>
                                                    <td id="text"> {{ application.codeSIERA }} </td>
                                            </tr>
                                            <tr>
                                                    <td id="label"> Nom de l'application dans SIERA </td>
                                                    <td id="text"> {{ application.nomApplicationSIERA }} </td>
                                            </tr>
                                            <tr>
                                                    <td id="label"> Application sous gouvernance </td> <td id="text"> 
                                                            {% if application.appliSousGouvernance %} Oui {% else %} Non {% endif %} 
                                                    </td>
                                            </tr>
                                            <tr>
                                                    <td id="label"> Inscrite au catalogue SIT </td>
                                                    <td id="text"> 
                                                            {% if application.dansCatalogueSIT %} Oui {% else %} Non {% endif %} 
                                                    </td>
                                            </tr>
                                            {#<tr>
                                                    <td id="label"> RefCatalogue </td>
                                                    <td id="text">
                                                            {{ application.refCatalogue.id }}
                                                    </td>
                                            </tr>#}
                                            <tr>
                                                    <td id="label"> Inscrite en revue de performance </td>
                                                    <td id="text"> 
                                                            {% if application.inscriteRevuePerformance %} Oui {% else %} Non {%endif %}
                                                    </td>
                                            </tr>
                                            <tr>
                                                    <td id="label"> OS cible </td>
                                                    <td> 
                                                            {{ application.oscible }}
                                                    </td>
                                            </tr>
                                            <tr> 
                                                    <td id="label"> Départements cibles </td>
                                                    <td id="read_only"> 
                                                            {% set i = 0 %}
                                                            {% for departement in application.populationCible %}
                                                                    {# compteur pour faire un retour à la ligne #}
                                                                    {% set i = i + 1 %}
                                                                    {% if i >= 2 %}
                                                                            </br>
                                                                    {% endif %}
                                                                    {{ departement.libelle }}
                                                            {% endfor %}
                                                    </td>
                                            </tr>
                                            {% if is_granted('IS_AUTHENTICATED_FULLY') %}
                                                {% if app.session and app.session.get('role') in ['Administrateur',
                                                                                                        'Responsable qualification',
                                                                                                        'Qualificateur',
                                                                                                        'Intégrateur',
                                                                                                        'Technicien support',
                                                                                                        'Chef de produit'
                                                                                                    ] %}
                                                    <tr>
                                                        <td id="label"> Assistance </td>
                                                        <td id="read_only">
                                                                {% set i = 0 %}
                                                                {% for agent in application.agents %}
                                                                        {# compteur pour faire un retour à la ligne #}
                                                                        {% set i = i + 1 %}
                                                                        {% if i >= 2 %}
                                                                                </br>
                                                                        {% endif %}
                                                                        {% if agent.role == 'assistance' %}
                                                                                {{ agent.libelle }}
                                                                        {% endif %}
                                                                {% endfor %}
                                                        </td>
                                                    </tr>
                                                    <tr> 
                                                        <td id="label"> Droits application </td> 
                                                        <td id="read_only"> 
                                                                {% set i = 0 %}
                                                                {% for agent in application.agents %}
                                                                        {# compteur pour faire un retour à la ligne #}
                                                                        {% set i = i + 1 %}
                                                                        {% if i >= 2 %}
                                                                                </br>
                                                                        {% endif %}
                                                                        {% if agent.role == 'ayantDroit' %}
                                                                                {{ agent.libelle }}
                                                                        {% endif %}
                                                                {% endfor %}
                                                        </td> 
                                                    </tr>
                                                    <tr> 
                                                        <td id="label"> Personnes habilitées pour l'installation </td>
                                                        <td> 
                                                            {% set i = 0 %}
                                                            {% for agent in application.agents %}
                                                                {# compteur pour faire un retour à la ligne #}
                                                                {% set i = i + 1 %}
                                                                {% if i >= 2 %}
                                                                        </br>
                                                                {% endif %}
                                                                {% if agent.role == 'personnes' %}
                                                                        {{ agent.libelle }}
                                                                {% endif %}
                                                            {% endfor %}
                                                        </td>
                                                    </tr>
                                                    <tr> 
                                                        <td id="label"> Groupes habilités pour l'installation</td>
                                                        <td> 
                                                            {% for groupeApplication in application.groupeApplications %}
                                                                <a href="{{path('rechercheUsersAD', {'groupeId': groupeApplication.groupe.id})}}" class="popup">{{ groupeApplication.groupe.libelle }}</a>
                                                            {% endfor %}
                                                        </td>
                                                    </tr>
                                                {% endif %}
                                            {% endif %}
                                    </table>
                                    </div>
                                </li>
                            {% endif %}
                            {% if(getAuthorisation('Pre-Requis/Non-Requis/Autres pre-requis ')) == true %}
                            <li>
                                    <a id="requis_accordion_plus" class="icone"> <img src="{{ asset('bundles/baquaras/images/plus.jpg') }}" height=20 width=20> </a> 
                                    <a id="requis_accordion_moins" class="icone"> <img src="{{ asset('bundles/baquaras/images/moins.jpg') }}" height=20 width=20> </a> 
                                    <h2> Pré-Requis/Non-Requis/Autres pré-requis </h2>

                                    <div id="requis">
                                            <div class="content" id="prerequis">
                                                    {% if application.preRequis is empty %}
                                                            <div class="text"> Pas de pré-requis </td>
                                                    {% endif %}

                                                    {% if application.preRequis is not empty %}

                                                            <h3> Liste des pré-requis </h3>

                                                            <table id="prerequis_liste">
                                                                    <tr>
                                                                            <th> Nom </th>
                                                                            <th> Mode de gestion </th>
                                                                            <th> OS </th>
                                                                    </tr>

                                                                    {% for preRequis in application.preRequis %}

                                                                            <tr>
                                                                                    <td> {{ preRequis.libelle }} </td>
                                                                                    <td> {{ preRequis.modeGestion }} </td>
                                                                                    <td> {{ preRequis.oscible }} </td>
                                                                            </tr>

                                                                    {% endfor %}

                                                            </table>

                                                    {% endif %}
                                            </div>

                                            <div class="content" id="nonrequis">
                                                    {% if application.nonRequis is empty %}
                                                            <div class="text"> Pas de non-requis </div>							
                                                    {% endif %}

                                                    {% if application.nonRequis is not empty %}

                                                            <h3> Liste des non-Requis </h3>

                                                            <table id="nonrequis_liste">
                                                                    <tr>
                                                                            <th> Nom </th>
                                                                            <th> Mode de gestion </th>
                                                                            <th> OS </th>
                                                                    </tr>

                                                                    {% for nonRequis in application.nonRequis %}						
                                                                            <tr>
                                                                                    <td> {{ nonRequis.libelle }} </td>
                                                                                    <td> {{ nonRequis.modeGestion }} </td>
                                                                                    <td> {{ nonRequis.oscible }} </td>
                                                                            </tr>
                                                                    {% endfor %}
                                                            </table>

                                                    {% endif %}
                                            </div>

                                            <div class="content" id="autreprerequis">
                                                    {% if application.autresPreRequis is empty %}
                                                            <div class="text"> Pas d'autres pré-requis </td>
                                                    {% endif %}

                                                    {% if application.autresPreRequis is not empty %}

                                                            <h3> Liste des autres pré-requis </h3>

                                                            <table id="autreprerequis_liste">
                                                                    <tr>
                                                                            <th> Libellé </th>
                                                                            <th> Commentaire </th>
                                                                            <th> OS </th>
                                                                    </tr>

                                                                    {% for autresPreRequis in application.autresPreRequis %}
                                                                            <tr>
                                                                                    <td> {{ autresPreRequis.libelle }} </td>
                                                                                    <td> {{ autresPreRequis.commentaire }} </td>
                                                                                    <td> {{ autresPreRequis.oscible }} </td>
                                                                            </tr>
                                                                    {% endfor  %}
                                                            </table>

                                                    {% endif %}
                                            </div>
                                    </div>
                            </li>
                        {% endif %}
                        {% if(getAuthorisation('Architecture')) == true %}
                            <li>
                                <a id="architecture_accordion_plus" class="icone"> <img src="{{ asset('bundles/baquaras/images/plus.jpg') }}" height=20 width=20> </a> 
                                <a id="architecture_accordion_moins" class="icone"> <img src="{{ asset('bundles/baquaras/images/moins.jpg') }}" height=20 width=20> </a>
                                <h2> Architecture </h2>

                                <div class="content" id="architecture">
                                <table>    					
                                        <tr> 
                                                <td id="label"> <div class="label"> Application web </div>
                                                        <a title="L'applicatione est-elle accessible au travers d'un navigateur internet ?" href="#"> <div class="bulle"> </div> </a>
                                                </td> 
                                                <td id="text">
                                                        {% if application.architecture.appliWeb is defined %}
                                                                {% if application.architecture.appliWeb is not null %}
                                                                        {% if application.architecture.appliWeb %} Oui {% else %} Non {% endif %}
                                                                {% endif %}
                                                        {% endif %}
                                                </td>
                                        </tr> 

                                        {% if application.architecture.appliWeb %}						
                                        <tr> 
                                                <td id="label"> URL Extranet </td>
                                                <td id="text"> 
                                                        {% if application.architecture.urlExtranet is defined %}
                                                                {{ application.architecture.urlExtranet }}
                                                        {% endif %}
                                                </td>
                                        </tr>	
                                        <tr> 
                                                <td id="label"> URL Intranet </td>
                                                <td id="text"> 
                                                        {% if application.architecture.urlIntranet is defined %}
                                                                {{ application.architecture.urlIntranet }}
                                                        {% endif %}
                                                </td>
                                        </tr>						
                                        <tr> 
                                                <td id="label"> Application intranet ou internet </td>
                                                <td> 
                                                        {% if application.architecture.appliIntraOuInternet is defined %} 
                                                                {{ application.architecture.appliIntraOuInternet }}
                                                        {% endif %}
                                                </td>
                                        </tr>					
                                        <tr> 
                                                <td id="label"> Utilisation d'ActiveX </td>
                                                <td>
                                                        {% if application.architecture.utilisationActiveX is defined %}
                                                                {% if application.architecture.utilisationActiveX is not null %}
                                                                        {% if application.architecture.utilisationActiveX %} Oui {% else %} Non {% endif %}
                                                                {% endif %}
                                                        {% endif %}
                                                </td> 
                                        </tr>					
                                        <tr> 
                                                <td id="label"> Utilisation de certificats </td>
                                                <td> 
                                                        {% if application.architecture.utilisationCertificats is defined %}
                                                                {% if application.architecture.utilisationCertificats is not null %}
                                                                        {% if application.architecture.utilisationCertificats %} Oui {% else %} Non {% endif %}
                                                                {% endif %}
                                                        {% endif %}
                                                </td>
                                        </tr>					
                                        <tr> 
                                                <td id="label"> Utilisation JRE </td>
                                                <td> 
                                                        {% if application.architecture.utilisationJRE is defined %}
                                                                {% if application.architecture.utilisationJRE is not null %}
                                                                        {% if application.architecture.utilisationJRE %} Oui {% else %} Non {% endif %}
                                                                {% endif %}
                                                        {% endif %}
                                                </td>
                                        </tr>
                                        <tr> 
                                                <td id="label"> Utilisation SSO </td>
                                                <td> 
                                                        {% if application.architecture.utilisationSSO is defined %}
                                                                {% if application.architecture.utilisationSSO is not null %}
                                                                        {% if application.architecture.utilisationSSO %} Oui {% else %} Non {% endif %}
                                                                {% endif %}
                                                        {% endif %}
                                                </td>
                                        </tr>
                                        {% endif %}
                                        <tr> 
                                                <td id="label"> <div class="label"> Application Client/Serveur </div>
                                                        <a title="Application nécessitant des interactions avec un(des) serveur(s)" href="#"> <div class="bulle"> </div> </a>
                                                </td> 
                                                <td> 
                                                        {% if application.architecture.appliClientServeur is defined %}
                                                                {% if application.architecture.appliClientServeur is not null %}
                                                                        {% if application.architecture.appliClientServeur %} Oui {% else %} Non {% endif %}
                                                                {% endif %}
                                                        {% endif %}
                                                </td>
                                        </tr>	
                                        <tr> 
                                                <td id="label"> Application infocentre </td>
                                                <td>
                                                        {% if application.architecture.appliInfocentre is defined %}
                                                                {% if application.architecture.appliInfocentre  is not null %}
                                                                        {% if application.architecture.appliInfocentre  %} Oui {% else %} Non {% endif %}
                                                                {% endif %}
                                                        {% endif %}
                                                </td>
                                        </tr>	
                                        <tr> 
                                                <td id="label"> <div class="label"> Base de données </div>
                                                        <a title="Si l'application nécessite l'accès à des bases de données autres que Oracle, cliquer sur oui" href="#"> <div class="bulle"> </div> </a>
                                                </td> 
                                                <td>
                                                        {% if application.architecture.baseDeDonnees is defined %}
                                                                {% if application.architecture.baseDeDonnees is not null %}
                                                                        {% if application.architecture.baseDeDonnees %} Oui {% else %} Non {% endif %}
                                                                {% endif %}
                                                        {% endif %}
                                                </td>
                                        </tr>	
                                        <tr> 
                                                <td id="label"> <div class="label"> Commentaire </div>
                                                        <a title="Mentionner tout commentaire utile à la prise de connaissance de l'application" href="#"> <div class="bulle"> </div> </a>
                                                </td> 
                                                <td> 
                                                        {% if application.architecture.commentaire is defined %}
                                                                {{ application.architecture.commentaire }}
                                                        {% endif %}
                                                </td>
                                        </tr>					
                                        <tr> 
                                                <td id="label"> Utilisation Office </td>
                                                <td>
                                                        {% if application.architecture.utilisationOffice is defined %}
                                                                {% if application.architecture.utilisationOffice is not null %}
                                                                        {% if application.architecture.utilisationOffice %} Oui {% else %} Non {% endif %}
                                                                {% endif %}
                                                        {% endif %}								
                                                </td>
                                        </tr>
                                        <tr> 
                                                <td id="label"> Liaison Office </td>
                                                <td> 
                                                        {% if application.architecture.liaisonsOffice is defined %}
                                                                {% for item in application.architecture.liaisonsOffice %}
                                                                        {{ item.libelle }} <br/>
                                                                {% endfor %}
                                                        {% endif %}
                                                </td>
                                        </tr>
                                        <tr> 
                                                <td id="label"> Liaison Access </td>
                                                <td> 
                                                        {% if application.architecture.liaisonAccess is defined %}
                                                                {{ application.architecture.liaisonAccess }} 
                                                        {% endif %}
                                                </td>
                                        </tr>
                                        <tr> 
                                                <td id="label"> Stocke les données utilisateurs </td>
                                                <td>
                                                        {% if application.architecture.stockeDonneesUser is defined %}
                                                                {% if application.architecture.stockeDonneesUser is not null %}
                                                                        {% if application.architecture.stockeDonneesUser %} Oui {% else %} Non {% endif %}
                                                                {% endif %}	
                                                        {% endif %}
                                                </td>
                                        </tr>
                                        <tr> 
                                                <td id="label"> Utilisation du matériel périphérique </td>
                                                <td> 
                                                        {% if application.architecture.utilisationMaterielPeripherique is defined %}
                                                                {% if application.architecture.utilisationMaterielPeripherique is not null %}
                                                                        {% if application.architecture.utilisationMaterielPeripherique %} Oui {% else %} Non {% endif %}
                                                                {% endif %}
                                                        {% endif %}
                                                </td>
                                        </tr>	
                                        <tr> 
                                                <td id="label"> Briques </td>
                                                <td> 
                                                        {% if application.architecture.briques is defined %}
                                                                {% if application.architecture.briques is not null %}
                                                                        {% for brique in application.architecture.briques %}
                                                                                {{ brique.libelle }}
                                                                        {% endfor %}
                                                                {% endif %}
                                                        {% endif %}
                                                </td> 
                                        </tr>						
                                </table>
                                </div>
                        </li>
                        {% endif %}
                        {% if(getAuthorisation('Gestion')) == true %}
                            <li>
                                    <a id="gestion_accordion_plus" class="icone"> <img src="{{ asset('bundles/baquaras/images/plus.jpg') }}" height=20 width=20> </a> 
                                    <a id="gestion_accordion_moins" class="icone"> <img src="{{ asset('bundles/baquaras/images/moins.jpg') }}" height=20 width=20> </a>
                                    <h2> Gestion </h2>

                                    <div class="content" id="gestion">						
                                    <table>	
                                        {% if is_granted('IS_AUTHENTICATED_FULLY') %}
                                            {% if app.session and app.session.get('role') in ['Administrateur',
                                                                                                'Responsable qualification',
                                                                                                'Qualificateur',
                                                                                                'Intégrateur',
                                                                                                'Chef de produit'
                                                                                            ] %}					
                                                    <tr>
                                                            <td id="label"> MOE </td>
                                                            <td id="read_only"> 
                                                                {#{ agent.libelle }#}
                                                            </td>
                                                    </tr>
                                                    <tr>
                                                        <td id="label"> Département MOE </td>
                                                        <td id="read_only"> SIT </td>
                                                    </tr>
                                                    <tr>
                                                        <td id="label"> MOA </td>
                                                        <td id="read_only"> 
                                                                {% for agent in application.agents %}
                                                                        {% if agent.role == 'moa' %}
                                                                                {{ agent.libelle }}
                                                                        {% endif %}
                                                                {% endfor %}
                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td id="label"> Département MOA </td>
                                                        <td id="read_only"> {{ application.deptMoa }}</td>
                                                    </tr>
                                                {% endif %}
                                                {% if app.session and app.session.get('role') in ['Administrateur',
                                                                                                'Responsable qualification',
                                                                                                'Qualificateur',
                                                                                                'Intégrateur',
                                                                                                'Chef de produit',
                                                                                                'Technicien support'
                                                                                            ] %}
                                                    <tr> 
                                                        <td id="label"> Liste des postes installés </td>
                                                        <td id="text"> {% if application.gestion.listePostesInstalles is defined %} {{ application.gestion.listePostesInstalles }} {% endif %} </td>
                                                    </tr>
                                                {% endif %}
                                                {% if app.session and app.session.get('role') in ['Administrateur',
                                                                                                'Responsable qualification',
                                                                                                'Qualificateur',
                                                                                                'Intégrateur',
                                                                                                'Chef de produit',
                                                                                            ] %}
                                                    <tr> 
                                                        <td id="label"> Postes pilotes WSUS </td>
                                                        <td id="text"> {% if application.gestion.postesPilotesWSUS is defined %} {{ application.gestion.postesPilotesWSUS }} {% endif %} </td>
                                                    </tr>
                                                {% endif %}
                                            {% endif %}
                                    </table>	
                                    </div>
                            </li>	
                            {% endif %}
                            {% if(getAuthorisation('Installation')) == true %}
                                <li>
                                        <a id="installation_accordion_plus" class="icone"> <img src="{{ asset('bundles/baquaras/images/plus.jpg') }}" height=20 width=20> </a> 
                                        <a id="installation_accordion_moins" class="icone"> <img src="{{ asset('bundles/baquaras/images/moins.jpg') }}" height=20 width=20> </a>
                                        <h2> Installation </h2> 

                                        <div class="content" id="installation">
                                        <table>	
                                            {% if is_granted('IS_AUTHENTICATED_FULLY') %}
                                                {% if app.session and app.session.get('role') in ['Administrateur',
                                                                                                    'Responsable qualification',
                                                                                                    'Qualificateur',
                                                                                                    'Intégrateur',
                                                                                                    'Chef de produit',
                                                                                                    'Technicien support'
                                                                                                ] %}
                                                    <tr> 
                                                            <td id="label"> <div class="label"> Comptabilisation license  </div>
                                                                    <a title="Y'at-il un reporting de réalisé suite à l'installation de l'application ? " href="#"> <div class="bulle"> </div> </a>
                                                            </td> 
                                                            <td id="text">
                                                                    {% if application.installation.comptabilisationLicense is defined %}
                                                                            {% if application.installation.comptabilisationLicense is not null %}
                                                                                    {% if application.installation.comptabilisationLicense %} Oui {% else %} Non {% endif %}
                                                                            {% endif %}
                                                                    {% endif %}
                                                            </td>
                                                    </tr>
                                                    <tr>
                                                            <td id="label"> <div class="label"> Nombre de licenses  </div>
                                                                    <a title="Nombre de licenses disponibles " href="#"> <div class="bulle"> </div> </a>
                                                            </td> 
                                                            <td id="text"> 
                                                                    {% if application.installation.nombreLicenses is defined %} 
                                                                            {{ application.installation.nombreLicenses }} 
